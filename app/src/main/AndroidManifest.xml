<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.vituniversity.hackathon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".NewBlog"
            android:label="New Blog">
            <intent-filter>
                <action android:name="com.vituniversity.hackathon.NewBlog"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".ViewBlog">
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="com.vitunviersity.hackathon.MainActivity"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="com.vitunviersity.hackathon.WelcomeActivity"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up"/>
        <activity
            android:name=".PayPalReg"
            android:label="Pay Pal"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.vitunviersity.hackathon.PayPalReg"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".NewsFeed">
            <intent-filter>
                <action android:name="com.vitunviersity.hackathon.NewsFeed"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>

</manifest>